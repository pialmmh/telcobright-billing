delimiter //
CREATE TRIGGER sum_voice_hr_01_after_insert
AFTER INSERT
   ON sum_voice_hr_01 FOR EACH ROW

BEGIN

   update cdrsummarymeta_hr_01
   set totalInsertedDuration=totalInsertedDuration+NEW.actualduration;

END;//
delimiter ;

